<!DOCTYPE html>
<html>
<head>
  <title>Tpe</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div id="user_nav">
	  <% if current_user %>
	    <%= link_to "Edit Profile", edit_user_path(:current) %> |
	    <%= link_to "Logout", logout_path %>
	  <% else %>
	    <%= link_to "Register", registration_path %> |
	    <%= link_to "Login", login_path %>
	  <% end %>
	</div>
<%= yield %>

</body>
</html>
